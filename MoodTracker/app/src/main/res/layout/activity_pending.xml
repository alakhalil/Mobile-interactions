                android:layout_marginRight="30dp"
                android:layout_weight="1"
                android:text="@string/bruce_spears"
                android:textColor="@color/black"
                android:textSize="14sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
